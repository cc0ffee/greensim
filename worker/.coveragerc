[run]
source = .
omit = 
    */tests/*
    */test_*.py
    */venv/*
    */__pycache__/*
    */site-packages/*
    */_internal/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod

[html]
directory = htmlcov

